   "source": [
    "def nta(n, bits=32):\n",
    "    bin_n = bin(n)[2:]\n",
    "    r = []\n",
    "    while bin_n:\n",
    "        r.append(hex(int(\"0b\" + bin_n[-bits:], 2)))\n",
    "        bin_n = bin_n[:-bits]\n",
    "    return r\n"
   ]
